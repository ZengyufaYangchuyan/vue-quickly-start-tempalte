{
  "name": "vue-quickly-start-tempalte",
  "sideEffects": false,
  "version": "1.0.0",
  "description": "this's an quickly start tempalte for vue",
  "main": "main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack.config.js --progress --colors",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.js --progress --colors",
    "build:dll": "webpack --config ./config/webpack/dll/index.js",
    "npmInstall": "npm install",
    "npmCachClean": "npm cache clean -f",
    "cnpmInstall": "cnpm install",
    "cnpmCachClean": "cnpm cache clean -f",
    "test": "npm run test",
    "gitCommit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZengyufaYangchuyan/vue-quickly-start-tempalte.git"
  },
  "keywords": [
    "vue"
  ],
  "author": "jzeng",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ZengyufaYangchuyan/vue-quickly-start-tempalte/issues"
  },
  "homepage": "https://github.com/ZengyufaYangchuyan/vue-quickly-start-tempalte#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/runtime-corejs2": "^7.9.2",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.1.2",
    "compression-webpack-plugin": "^4.0.0",
    "conventional-changelog": "^3.1.21",
    "conventional-changelog-cli": "^2.0.34",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.0",
    "css-loader": "^3.5.3",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^6.8.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-vue": "^7.0.0-alpha.1",
    "file-loader": "^6.0.0",
    "gitmoji-cli": "^3.2.4",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.5",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
    "postcss-sprites": "^4.2.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "ts-loader": "^7.0.1",
    "typescript": "^3.8.3",
    "url-loader": "^4.1.0",
    "vue-loader": "^15.9.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "axios": "^0.19.2",
