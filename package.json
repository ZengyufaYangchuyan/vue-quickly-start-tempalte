{
  "name": "vue-quickly-start-tempalte",
  "sideEffects": false,
  "version": "1.0.0",
  "description": "this's an quickly start tempalte for vue",
  "main": "main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack/index.js --open --progress --colors --devtool inline-source-map --hot",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack/index.js --progress --colors --devtool cheap-module-source-map",
    "build:dll": "webpack --config ./config/webpack/dll/index.js",
    "test": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZengyufaYangchuyan/vue-quickly-start-tempalte.git"
  },
  "keywords": [
    "vue"
  ],
  "author": "jzeng",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ZengyufaYangchuyan/vue-quickly-start-tempalte/issues"
  },
  "homepage": "https://github.com/ZengyufaYangchuyan/vue-quickly-start-tempalte#readme",
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.0",
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
